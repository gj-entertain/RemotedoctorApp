/* pages/automationDiagnosis/automationDiagnosis.wxss */
@import "stylesheet.wxss";
.head {
  height: 210rpx;
  width: 100%;
  /* margin-bottom:80rpx; */
  display: relative;
  align-items: center;
  background-color:#FCE2A9;
  padding-left:25rpx;
}
.empty {
  height: 70rpx;
  width: 100%;
  /* margin-bottom:80rpx; */
  display: relative;
  align-items: center;
  background-color:#FCE2A9;
}

.head text{
  font-size:25pt;
  font-weight:bolder;
  line-height: 60rpx;
}

.iconfont{
  font-size:40rpx;
}

button::after{
  border: none;
}

button{
  background-color: #585858;
  border-radius:0;
  color: white;
}

.wepy{
  margin-left:auto;
  margin-right:auto;
  margin-top: 20rpx;
  height: 100rpx;
  background: #585858;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
  text-align: center;
  color: white;
  width:90%;
}
.wrapper{
  width:90%;
  margin-left:auto;
  margin-right:auto;
  margin-top:20rpx;
  border-radius: 30rpx;
}

.submitBtn{
  text-align: center;
  color: white;
  width:90%;
  border-radius: 30rpx;
  margin-top:30rpx;
}

.input{
  display:inline-block;
  width:80%;
  padding-left:20rpx;
}

.search{
  padding-left:20rpx;
  height:80rpx;
  padding-top:40rpx;
  box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.2);
  border: 1px solid rgba(0, 0, 0, 0.1);
}