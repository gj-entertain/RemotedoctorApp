@import "stylesheet.wxss";


.head {
  height: 210rpx;
  width: 100%;
  /* margin-bottom:80rpx; */
  display: relative;
  align-items: center;
  background-color:#FCE2A9;
  padding-left:25rpx;
}
.empty {
  height: 70rpx;
  width: 100%;
  /* margin-bottom:80rpx; */
  display: relative;
  align-items: center;
  background-color:#FCE2A9;
}

.head text{
  font-size:25pt;
  font-weight:bolder;
  line-height: 60rpx;
}

.search{
  margin-left:auto;
  margin-right:auto;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
  height: 290rpx;
  width: 290rpx;
  background: #585858;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
  border-radius: 20%;
  box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.2);
  font-weight: bold;
  text-align: center;
}

.icon-chakan{
  font-size: 70rpx;
  color: white;
}

button::after{
  border: none;
}

button{
  background-color: #585858;
  color: white;
  border-radius: 30rpx;
  width:90%;
  margin-bottom:20rpx;
}
