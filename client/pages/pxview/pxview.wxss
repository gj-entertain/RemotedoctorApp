@import "../automationDiagnosis/stylesheet.wxss";

page {  
  /* display: block;   */
  min-height: 100%;  
  background-color: #FCE2A9;  
}  


.title{
  width: 100%;
  line-height: 45px;
  font-size: 50rpx;
  text-align: center;
  align-content: center;
  font-weight: bold;
  padding-top:20rpx;
}

.container{
  align-items: center;
}

.record{
  margin-left:auto;
  margin-right:auto;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
  /* height: 400rpx; */
  width: 700rpx;
  background: #fff;
  transition: all 300ms ease;
  box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.2);
  border-radius:10rpx;
  padding-left:10rpx;
}

.data{
  line-height: 25px;
  margin-left: 20rpx;
  font-size: 30rpx;
}

.date{
  color: white;
  background-color:#585858;
  padding:5rpx;
  padding-left:10rpx;
}

.loginBtn{
  width:90%;
  border-radius: 30rpx;
}

.search{
  padding-left:20rpx;
  height:80rpx;
  padding-top:30rpx;
  box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.2);
  border: 1px solid rgba(0, 0, 0, 0.1);
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  width:90%;
  margin-left:auto;
  margin-right:auto;
  background-color: white;
  border-radius: 30rpx;
}

.inputGoods{
  display:inline-block;
  width:80%;
  padding-left:20rpx;
}
